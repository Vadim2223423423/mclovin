print ("pisyapopakaka")

num1 = float(input("Введите первое число: "))
operator = input("Введите оператор (+, -, *, /): ")
num2 = float(input("Введите второе число: "))

if operator == "+":
    result = num1 + num2
elif operator == "-":
    result = num1 - num2
elif operator == "*":
    result = num1 * num2
elif operator == "/":
     result = num1 / num2
    print("Результат:", result)
elif operator == "**":
    result = num1 * num2
